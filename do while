package javaapplication3;

/**
 *
 * @author PC03-LAB02
 */
public class JavaApplication3 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int n=10;
        int c=1;
        do{
            System.out.println(c);
            c=c+1;           
        }while (c<=n);
    }
    
}
